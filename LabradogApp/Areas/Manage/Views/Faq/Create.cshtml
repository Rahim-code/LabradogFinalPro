@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Manage/Views/Shared/_Layout.cshtml";
}

<form class="container" enctype="multipart/form-data" method="post" action="@Url.Action("create","faq")">
    <div class="form-group">
        <input type="text" class="form-control form-control-user"
               id="Question" name="Question" aria-describedby="emailHelp"
               placeholder="Enter Question..." required>
    </div>
    <div class="form-group">
        <input type="text" class="form-control form-control-user"
               id="Answer" name="Answer" aria-describedby="emailHelp"
               placeholder="Enter Answer..." required>
    </div>
    <button type="submit" class="btn btn-primary btn-user btn-block">
        Add This Fag
    </button>
    <hr>
</form>
